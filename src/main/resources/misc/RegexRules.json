[
  {
    "rule": "^[GCDZTSPKXLY1-9]\\d{1,4}$",
    "title": "火车车次",
    "examples": [
      "G1868",
      "D102",
      "D9",
      "Z5",
      "Z24",
      "Z17"
    ]
  },
  {
    "rule": "^\\d{15,17}$",
    "title": "手机机身码(IMEI)",
    "examples": [
      "123456789012345",
      "1234567890123456",
      "12345678901234567"
    ]
  },
  {
    "rule": "^((ht|f)tps?:\\/\\/)?[\\w-]+(\\.[\\w-]+)+:\\d{1,5}\\/?$",
    "title": "必须带端口号的网址(或ip)",
    "examples": [
      "https://www.qq.com:8080",
      "127.0.0.1:5050",
      "baidu.com:8001",
      "http://192.168.1.1:9090"
    ]
  },
  {
    "rule": "^(((ht|f)tps?):\\/\\/)?[\\w-]+(\\.[\\w-]+)+([\\w.,@?^=%&:/~+#-]*[\\w@?^=%&/~+#-])?$",
    "title": "网址(url,支持端口和\"?+参数\"和\"#+参数)",
    "examples": [
      "www.qq.com",
      "https://baidu.com",
      "360.com:8080/vue/#/a=1&b=2"
    ]
  },
  {
    "rule": "^[0-9A-HJ-NPQRTUWXY]{2}\\d{6}[0-9A-HJ-NPQRTUWXY]{10}$",
    "title": "统一社会信用代码",
    "examples": [
      "91230184MA1BUFLT44",
      "92371000MA3MXH0E3W"
    ]
  },
  {
    "rule": "^(([0-9A-Za-z]{15})|([0-9A-Za-z]{18})|([0-9A-Za-z]{20}))$",
    "title": "统一社会信用代码(宽松匹配)(15位/18位/20位数字/字母)",
    "examples": [
      "91110108772551611J",
      "911101085923662400"
    ]
  },
  {
    "rule": "^thunderx?:\\/\\/[a-zA-Z\\d]+=$",
    "title": "迅雷链接",
    "examples": [
      "thunder://QUEsICdtYWduZXQ6P3h0PXVybjpidGloOjBCQTE0RTUxRkUwNjU1RjE0Qzc4NjE4RjY4NDY0QjZFNTEyNjcyOUMnWlo="
    ]
  },
  {
    "rule": "^ed2k:\\/\\/\\|file\\|.+\\|\\/$",
    "title": "ed2k链接(宽松匹配)",
    "examples": [
      "ed2k://|file|%E5%AF%84%E7%94%9F%E8%99%AB.PARASITE.2019.HD-1080p.X264.AAC-UUMp4(ED2000.COM).mp4|2501554832|C0B93E0879C6071CBED732C20CE577A3|h=5HTKZPQFYRKORN52I3M7GQ4QQCIHFIBV|/"
    ]
  },
  {
    "rule": "^magnet:\\?xt=urn:btih:[0-9a-fA-F]{40,}.*$",
    "title": "磁力链接(宽松匹配)",
    "examples": [
      "magnet:?xt=urn:btih:40A89A6F4FB1498A98087109D012A9A851FBE0FC"
    ]
  },
  {
    "rule": "^(?:\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])(?:\\.(?:\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])){3}$",
    "title": "子网掩码",
    "examples": [
      "255.255.255.0",
      "255.224.0.0"
    ]
  },
  {
    "rule": "^\\/(?:[^/]+\\/)*\\.[^/]*",
    "title": "linux\"隐藏文件\"路径",
    "examples": [
      "/usr/ad/.dd",
      "/root/.gitignore",
      "/.gitignore"
    ]
  },
  {
    "rule": "^\\/(?:[^/]+\\/)*$",
    "title": "linux文件夹路径",
    "examples": [
      "/usr/ad/dd/",
      "/",
      "/root/"
    ]
  },
  {
    "rule": "^\\/(?:[^/]+\\/)*[^/]+$",
    "title": "linux文件路径",
    "examples": [
      "/root/b.ts",
      "/root/abc"
    ]
  },
  {
    "rule": "^[a-zA-Z]:\\\\(?:\\w+\\\\?)*$",
    "title": "window\"文件夹\"路径",
    "examples": [
      "C:\\Users\\Administrator\\Desktop",
      "e:\\m\\"
    ]
  },
  {
    "rule": "^[a-zA-Z]:\\\\(?:\\w+\\\\)*\\w+\\.\\w+$",
    "title": "window下\"文件\"路径",
    "examples": [
      "C:\\Users\\Administrator\\Desktop\\qq.link",
      "e:\\m\\vscode.exe"
    ]
  },
  {
    "rule": "^(s[hz]|S[HZ])(000[\\d]{3}|002[\\d]{3}|300[\\d]{3}|600[\\d]{3}|60[\\d]{4})$",
    "title": "股票代码(A股)",
    "examples": [
      "sz000858",
      "SZ002136",
      "sz300675",
      "SH600600",
      "sh601155"
    ]
  },
  {
    "rule": "^150$|^(?:\\d|[1-9]\\d|1[0-4]\\d)(?:.5)?$",
    "title": "大于等于0, 小于等于150, 支持小数位出现5, 如145.5, 用于判断考卷分数",
    "examples": [
      "150",
      "100.5"
    ]
  },
  {
    "rule": "^<!--[\\s\\S]*?-->$",
    "title": "html注释",
    "examples": [
      "<!--<div class=\"_bubble\"><\/div>-->"
    ]
  },
  {
    "rule": "^([a-f\\d]{32}|[A-F\\d]{32})$",
    "title": "md5格式(32位)",
    "examples": [
      "21fe181c5bfc16306a6828c1f7b762e8"
    ]
  },
  {
    "rule": "^\\d+(?:\\.\\d+){2}$",
    "title": "版本号(version)格式必须为X.Y.Z",
    "examples": [
      "16.3.10"
    ]
  },
  {
    "rule": "^(?i)https?:\\/\\/(.+\\/)+.+(\\.(swf|avi|flv|mpg|rm|mov|wav|asf|3gp|mkv|rmvb|mp4))$",
    "title": "视频(video)链接地址（视频格式可按需增删）",
    "examples": [
      "http://www.abc.com/video/wc.avi"
    ]
  },
  {
    "rule": "^(?i)https?:\\/\\/(.+\\/)+.+(\\.(gif|png|jpg|jpeg|webp|svg|psd|bmp|tif))$",
    "title": "图片(image)链接地址（图片格式可按需增删）",
    "examples": [
      "https://www.abc.com/logo.png"
    ]
  },
  {
    "rule": "^(?:[01]\\d|2[0-3]):[0-5]\\d:[0-5]\\d$",
    "title": "24小时制时间（HH:mm:ss）",
    "examples": [
      "23:34:55"
    ]
  },
  {
    "rule": "^(?:1[0-2]|0?[1-9]):[0-5]\\d:[0-5]\\d$",
    "title": "12小时制时间（hh:mm:ss）",
    "examples": [
      "11:34:55"
    ]
  },
  {
    "rule": "^(?i)\\s*data:(?:[a-z]+\\/[a-z0-9-+.]+(?:;[a-z-]+=[a-z0-9-]+)?)?(?:;base64)?,([a-z0-9!$&',()*+;=\\-._~:@/?%\\s]*?)\\s*$",
    "title": "base64格式",
    "examples": [
      "data:image/gif;base64,xxxx=="
    ]
  },
  {
    "rule": "^-?\\d+(,\\d{3})*(\\.\\d{1,2})?$",
    "title": "数字/货币金额（支持负数、千分位分隔符）",
    "examples": [
      "100",
      "-0.99",
      "3",
      "234.32",
      "-1",
      "900",
      "235.09",
      "12,345,678.90"
    ]
  },
  {
    "rule": "(?:^[1-9]([0-9]+)?(?:\\.[0-9]{1,2})?$)|(?:^(?:0){1}$)|(?:^[0-9]\\.[0-9](?:[0-9])?$)",
    "title": "数字/货币金额 (只支持正数、不支持校验千分位分隔符)",
    "examples": [
      "0.99",
      "8.99",
      "666"
    ]
  },
  {
    "rule": "^[1-9]\\d{9,29}$",
    "title": "银行卡号（10到30位, 覆盖对公/私账户, 参考[微信支付](https://pay.weixin.qq.com/wiki/doc/api/xiaowei.php?chapter=22_1)）",
    "examples": [
      "6234567890",
      "6222026006705354000"
    ]
  },
  {
    "rule": "^(?:[\\u4e00-\\u9fa5·]{2,16})$",
    "title": "中文姓名",
    "examples": [
      "葛二蛋",
      "凯文·杜兰特",
      "德克·维尔纳·诺维茨基"
    ]
  },
  {
    "rule": "(^[a-zA-Z]{1}[a-zA-Z\\s]{0,20}[a-zA-Z]{1}$)",
    "title": "英文姓名",
    "examples": [
      "James",
      "Kevin Wayne Durant",
      "Dirk Nowitzki"
    ]
  },
  {
    "rule": "^[京津沪渝冀豫云辽黑湘皖鲁新苏浙赣鄂桂甘晋蒙陕吉闽贵粤青藏川宁琼使领]{1}[A-HJ-NP-Z]{1}(([0-9]{5}[DF])|([DF][A-HJ-NP-Z0-9][0-9]{4}))$",
    "title": "车牌号(新能源)",
    "examples": [
      "京AD92035",
      "甘G23459F"
    ]
  },
  {
    "rule": "^[京津沪渝冀豫云辽黑湘皖鲁新苏浙赣鄂桂甘晋蒙陕吉闽贵粤青藏川宁琼使领]{1}[A-HJ-NP-Z]{1}[A-HJ-NP-Z0-9]{4}[A-HJ-NP-Z0-9挂学警港澳]{1}$",
    "title": "车牌号(非新能源)",
    "examples": [
      "京A00599",
      "黑D23908"
    ]
  },
  {
    "rule": "^[京津沪渝冀豫云辽黑湘皖鲁新苏浙赣鄂桂甘晋蒙陕吉闽贵粤青藏川宁琼使领]{1}[A-HJ-NP-Z]{1}(?:(([0-9]{5}[DF])|([DF][A-HJ-NP-Z0-9][0-9]{4}))|[A-HJ-NP-Z0-9]{4}[A-HJ-NP-Z0-9挂学警港澳]{1})$",
    "title": "车牌号(新能源+非新能源)",
    "examples": [
      "京A12345D",
      "京A00599"
    ]
  },
  {
    "rule": "^(?:(?:\\+|00)86)?1(?:(?:3[\\d])|(?:4[5-7|9])|(?:5[0-3|5-9])|(?:6[5-7])|(?:7[0-8])|(?:8[\\d])|(?:9[1|8|9]))\\d{8}$",
    "title": "手机号(mobile phone)中国(严谨), 根据工信部2019年最新公布的手机号段",
    "examples": [
      "008618311006933",
      "+8617888829981",
      "19119255642"
    ]
  },
  {
    "rule": "^(?:(?:\\+|00)86)?1[3-9]\\d{9}$",
    "title": "手机号(mobile phone)中国(宽松), 只要是13,14,15,16,17,18,19开头即可",
    "examples": [
      "008618311006933",
      "+8617888829981",
      "19119255642"
    ]
  },
  {
    "rule": "^(?:(?:\\+|00)86)?1\\d{10}$",
    "title": "手机号(mobile phone)中国(最宽松), 只要是1开头即可, 如果你的手机号是用来接收短信, 优先建议选择这一条",
    "examples": [
      "008618311006933",
      "+8617888829981",
      "19119255642"
    ]
  },
  {
    "rule": "^\\d{4}(-)(1[0-2]|0?\\d)\\1([0-2]\\d|\\d|30|31)$",
    "title": "date(日期)",
    "examples": [
      "1990-12-12",
      "2020-1-1"
    ]
  },
  {
    "rule": "^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$",
    "title": "email(邮箱)",
    "examples": [
      "90203918@qq.com",
      "nbilly@126.com"
    ]
  },
  {
    "rule": "^\\d{3}-\\d{8}$|^\\d{4}-\\d{7,8}$",
    "title": "座机(tel phone)电话(国内),如: 0341-86091234",
    "examples": [
      "0936-4211235"
    ]
  },
  {
    "rule": "^(?:\\d{3}-)?\\d{8}$|^(?:\\d{4}-)?\\d{7,8}$",
    "title": "座机(tel phone)电话(国内), 区号可选,如: 0341-86091234",
    "examples": [
      "0936-4211235",
      "89076543"
    ]
  },
  {
    "rule": "^[1-9]\\d{7}(?:0\\d|10|11|12)(?:0[1-9]|[1-2][\\d]|30|31)\\d{3}$",
    "title": "身份证号(1代,15位数字)",
    "examples": [
      "123456991010193"
    ]
  },
  {
    "rule": "^[1-9]\\d{5}(?:18|19|20)\\d{2}(?:0[1-9]|10|11|12)(?:0[1-9]|[1-2]\\d|30|31)\\d{3}[\\dXx]$",
    "title": "身份证号(2代,18位数字),最后一位是校验位,可能为数字或字符X",
    "examples": [
      "12345619991205131x"
    ]
  },
  {
    "rule": "(^\\d{8}(0\\d|10|11|12)([0-2]\\d|30|31)\\d{3}$)|(^\\d{6}(18|19|20)\\d{2}(0[1-9]|10|11|12)([0-2]\\d|30|31)\\d{3}(\\d|X|x)$)",
    "title": "身份证号, 支持1/2代(15位/18位数字)",
    "examples": [
      "622223199912051311"
    ]
  },
  {
    "rule": "(^[EeKkGgDdSsPpHh]\\d{8}$)|(^(([Ee][a-fA-F])|([DdSsPp][Ee])|([Kk][Jj])|([Mm][Aa])|(1[45]))\\d{7}$)",
    "title": "护照（包含香港、澳门）",
    "examples": [
      "s28233515",
      "141234567",
      "159203084",
      "MA1234567",
      "K25345719"
    ]
  },
  {
    "rule": "^[a-zA-Z]\\w{4,15}$",
    "title": "帐号是否合法(字母开头，允许5-16字节，允许字母数字下划线组合",
    "examples": [
      "justin",
      "justin1989",
      "justin_666"
    ]
  },
  {
    "rule": "^(?:[\\u3400-\\u4DB5\\u4E00-\\u9FEA\\uFA0E\\uFA0F\\uFA11\\uFA13\\uFA14\\uFA1F\\uFA21\\uFA23\\uFA24\\uFA27-\\uFA29]|[\\uD840-\\uD868\\uD86A-\\uD86C\\uD86F-\\uD872\\uD874-\\uD879][\\uDC00-\\uDFFF]|\\uD869[\\uDC00-\\uDED6\\uDF00-\\uDFFF]|\\uD86D[\\uDC00-\\uDF34\\uDF40-\\uDFFF]|\\uD86E[\\uDC00-\\uDC1D\\uDC20-\\uDFFF]|\\uD873[\\uDC00-\\uDEA1\\uDEB0-\\uDFFF]|\\uD87A[\\uDC00-\\uDFE0])+$",
    "title": "中文/汉字",
    "examples": [
      "正则",
      "前端"
    ]
  },
  {
    "rule": "^\\d+\\.\\d+$",
    "title": "小数",
    "examples": [
      "0.0",
      "0.09"
    ]
  },
  {
    "rule": "^\\d{1,}$",
    "title": "数字",
    "examples": [
      "12345678"
    ]
  },
  {
    "rule": "<(\\w+)[^>]*>(.*?<\\/\\1>)?",
    "title": "html标签(宽松匹配)",
    "examples": [
      "<div id=\"app\"> 2333 <\/div>",
      "<input type=\"text\">",
      "<br>"
    ]
  },
  {
    "rule": "^[1-9][0-9]{4,10}$",
    "title": "qq号格式正确",
    "examples": [
      "903013545",
      "9020304"
    ]
  },
  {
    "rule": "^[A-Za-z0-9]+$",
    "title": "数字和字母组成",
    "examples": [
      "james666",
      "haha233hi"
    ]
  },
  {
    "rule": "^[a-zA-Z]+$",
    "title": "英文字母",
    "examples": [
      "Russel"
    ]
  },
  {
    "rule": "^[a-z]+$",
    "title": "小写英文字母组成",
    "examples": [
      "russel"
    ]
  },
  {
    "rule": "^[A-Z]+$",
    "title": "大写英文字母",
    "examples": [
      "ABC",
      "KD"
    ]
  },
  {
    "rule": "^\\S*(?=\\S{6,})(?=\\S*\\d)(?=\\S*[A-Z])(?=\\S*[a-z])(?=\\S*[!@#$%^&*? ])\\S*$",
    "title": "密码强度校验，最少6位，包括至少1个大写字母，1个小写字母，1个数字，1个特殊字符",
    "examples": [
      "Kd@curry666"
    ]
  },
  {
    "rule": "^[a-zA-Z0-9_-]{4,16}$",
    "title": "用户名校验，4到16位（字母，数字，下划线，减号）",
    "examples": [
      "xiaohua_qq"
    ]
  },
  {
    "rule": "^((\\d|[1-9]\\d|1\\d\\d|2[0-4]\\d|25[0-5]).){3}(\\d|[1-9]\\d|1\\d\\d|2[0-4]\\d|25[0-5])(?::(?:[0-9]|[1-9][0-9]{1,3}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5]))?$",
    "title": "ip-v4[:端口]",
    "examples": [
      "172.16.0.0",
      "172.16.0.0:8080",
      "127.0.0.0",
      "127.0.0.0:998"
    ]
